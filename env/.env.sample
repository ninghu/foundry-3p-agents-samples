# Common azd environment configuration values
AZURE_SUBSCRIPTION_ID=<your-subscription-id>
AZURE_ENV_NAME=agent-sample
AZURE_LOCATION=westus3

# Agent runtime settings passed to the container
AZURE_AI_PROJECT_ENDPOINT=https://<your-project>.services.ai.azure.com/api/projects/<project-id>
AZURE_AI_MODEL_DEPLOYMENT_NAME=gpt-4o-mini
# Optional: use an existing persistent agent instead of creating ephemeral ones
AZURE_AI_AGENT_ID=
