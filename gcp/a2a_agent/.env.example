# --- Model selection ---
model_source=google  # set to azure for Azure OpenAI or any other value for the OpenAI-compatible path

# --- Gemini model configuration (default path) ---
GOOGLE_API_KEY=your-gemini-api-key
GOOGLE_MODEL_NAME=gemini-2.0-flash

# --- Optional: Azure OpenAI configuration ---
# model_source=azure
# AZURE_OPENAI_ENDPOINT=https://your-azure-resource.openai.azure.com/
# AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini
# AZURE_OPENAI_API_KEY=your-azure-openai-key
# AZURE_OPENAI_API_VERSION=2024-08-01-preview

# --- Optional: OpenAI-compatible endpoint configuration ---
# TOOL_LLM_URL=https://your-endpoint/v1
# TOOL_LLM_NAME=gpt-4o-mini
# API_KEY=your-openai-style-key

# --- Optional: Azure Application Insights telemetry ---
# APPLICATION_INSIGHTS_CONNECTION_STRING=InstrumentationKey=...
# APPLICATION_INSIGHTS_AGENT_NAME=gcp-a2a-currency-agent
# APPLICATION_INSIGHTS_AGENT_ID=gcp-a2a-currency-agent
# APPLICATION_INSIGHTS_PROVIDER_NAME=gcp.a2a
# APPLICATION_INSIGHTS_ENABLE_CONTENT=true

# --- API access guard ---
A2A_AGENT_API_KEY=your-agent-api-key

# --- Google Cloud deployment ---
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1

# --- Optional service metadata overrides ---
# PUBLIC_HOST=localhost
# PUBLIC_PORT=8080
# PUBLIC_SCHEME=http
# BIND_HOST=0.0.0.0
